<h1 class="title is-6">My Events</h1>


<% @user.created_events.each do |event| %>
  <div class="box">
    <article class="media">
      <div class="content">
        <%= link_to event, class: 'has-text-grey' do %>
          <p><%= event.title %></p>
        <% end %>
      </div>
    </article>
  </div>
<% end %>
